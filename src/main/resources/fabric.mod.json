{
  "schemaVersion": 1,
  "id": "tesseract",
  "version": "${version}",

  "name": "Tesseract API",
  "description": "A lightweight dimensional API for disposable, low-overhead pocket dimensions.",
  "authors": [ "Vatuu" ],

  "contact": {
    "homepage": "https://curseforge.com/minecraft/mc-mods/tesseract-api",
    "sources": "https://github.com/vatuu/tesseract"
  },

  "license": "MIT",
  "icon": "assets/tesseract/icon.png",
  "custom": {
    "modmenu:api": true
  },

  "accessWidener": "tesseract.widener",

  "entrypoints": {
    "main": [ "dev.vatuu.tesseract.Tesseract" ],
    "client": [ "dev.vatuu.tesseract.client.TesseractClient" ]
  },
  "mixins": [
    "mixins.tesseract.json",
    "mixins.client.tesseract.json"
  ],

  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric-events-lifecycle-v0": ">=0.1.1"
